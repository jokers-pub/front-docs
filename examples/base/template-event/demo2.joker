<template>
    <button @click="handleClick('I am the parameter passed by the execution event')">Click me!</button>
</template>
<script>
import { Component, VNode } from "@joker.front/core";
import { MessageBox } from "@joker.front/ui";

export default class extends Component {
    handleClick(e: VNode.Event, param: string) {
        MessageBox.alert(param);
    }
}
</script>